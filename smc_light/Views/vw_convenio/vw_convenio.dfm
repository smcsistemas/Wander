  object sql_conveniados: TFDQuery
    Connection = Module.connection
    SQL.Strings = (
      
        'select cc.id, concat(cc.nome, '#39' (CPF: '#39', cc.cpf, '#39' - Matr'#237'cula: ' +
        #39', cc.matricula, '#39')'#39') as conveniado from cliente_conveniado cc'
      
        'where cc.cliente_id = :pcliente_id and status = 1 and current_da' +
        'te() between periodo_inicio and periodo_fim')
    Left = 280
    Top = 32
    ParamData = <
