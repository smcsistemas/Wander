class function TDB.Backup(pDestino: string): string;
var
  bComand: string;
  h: thandle;
  SEI: TShellExecuteInfo; // ponteiro
  exitcode: dword;
begin
  try
    if pDestino = '' then
      pDestino := TDir.temp.root;

    pDestino := '"' + pDestino + 'db_' + FormatDateTime('dd_mm_yyyy_HH_MM', Now) + '.sql"';
    result := pDestino;
    TFunctions.ExecuteCommand(TDir._MysqlDump + ' -h' + tEnv.database.local.server + ' -u' + tEnv.database.local.user + ' -p' + tEnv.database.local.password +
      ' -c -e --databases ' + tEnv.database.local.database + ' --result-file=' + pDestino);
    sleep(1000);

  except
    on e: exception do
      TFunctions.write_Log('Erro backup DB: ' + e.Message);
  end;
end;

class function TDB.GenIn(const arr_true: array of boolean; const arr_values: array of Variant): string;
var
  i: integer;
  arr_in: tStringList;
begin

  result := '';
  arr_in := tStringList.Create;
  for i := Low(arr_values) to High(arr_values) do
  begin
    if arr_true[i] then
      if arr_values[i] = NULL then
        arr_in.Add('Null')
      else
      begin
        if VarType(arr_values[i]) in [varDouble, varInteger] then
          arr_in.Add(arr_values[i])
        else
          arr_in.Add('"' + VarToStr(arr_values[i]) + '"')
      end;
  end;

  result := TFunctions.arrToStr(arr_in);
  if result <> '' then
    result := '(' + result + ')'
  else
    result := '("")';
end;

class function TDB.GenIn(const arr_values: array of Variant): string;
var
  arrBol: tlist<boolean>;
  i: integer;
begin

  arrBol := tlist<boolean>.Create;
  arrBol.Clear;

  for i := Low(arr_values) to High(arr_values) do
    arrBol.Add(True);

  result := TDB.GenIn(arrBol.toarray(), arr_values);

end;

class function TDB.GenWhere(const FieldNames: array of string): string;
var
  filter: string;
  i: integer;
begin

  filter := ' WHERE ';
  for i := Low(FieldNames) to High(FieldNames) do
  begin
    filter := concat(filter, ' ', FieldNames[i], '=?');
    if i <> High(FieldNames) then
      filter := concat(filter, ' AND');
  end;
  result := filter;
end;

class function TDB.removeSqlInjection(value: string): string;
begin
  result := value;
  result := TFunctions.replace(result, '"');
  result := TFunctions.replace(result, '--');
  result := TFunctions.replace(result, ';');
end;

class function TDB.ColumnExists(ColumnName, TableName: string): boolean;
begin
  result := SimpleQuery('select column_name from information_schema.columns where column_name = ? and table_name = ? and table_schema="' + Connect(dbLocal)
    .Params.Values['database'] + '"', [ColumnName, TableName]) <> nil;
end;

class function TDB.IndexExists(IndexName: string): boolean;
begin
  result := SimpleQuery('select * from information_schema.STATISTICS where INDEX_NAME = ? and table_schema="' + Connect(dbLocal).Params.Values['database'] + '"', [IndexName]) <> nil;
end;

class function TDB.TableExists(TableName: string): boolean;
begin
  result := SimpleQuery('select table_name from information_schema.tables where table_name = ? and table_schema="' + Connect(dbLocal).Params.Values['database']
    + '"', [TableName]) <> nil;
end;

