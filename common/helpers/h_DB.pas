class function TDB.SQLExec(const pSqlType: TSQLType; const query: String; arr_params: array of Variant; pConn: TDBConn): TFDQuery;
var
  i: integer;
begin
  try
    result := TFDQuery.Create(nil);
    with result do
    begin
      connection := Connect(pConn);
      active := false;
      close;
      sql.Clear;
      sql.Add(query);
      for i := Low(arr_params) to High(arr_params) do
        Params[i].value := arr_params[i];
      case pSqlType of
        TSQLType.TSqlReadData:
          begin
            open;
            active := True;
            FetchAll;
            if recordcount = 0 then
              result := nil;
          end;
        TSQLType.TSqlManipulateData:
          ExecSQL;
      end;
    end;
  except
    on e: exception do
    begin
      TFunctions.write_Log(e.Message);
      raise exception.Create(e.Message);
    end;
  end;
end;

class function TDB.SimpleQueryWithoutGui(const query: String): TFDQuery;
var
  qry: TFDQuery;
begin
  result := nil;
  try
    qry := TFDQuery.Create(nil);
    with qry do
    begin
      connection := Connect(TDBConn.dbLocal, gcrnone);
      close;
      sql.Clear;
      open(query);
      FetchAll;
      if recordcount <> 0 then
        result := qry;
    end;
  except
    on e: exception do
      TFunctions.write_Log(e.Message);
  end;
end;

class function TDB.SimpleQuery(const query: String; pConn: TDBConn = dbLocal): TFDQuery;
begin
  result := self.SimpleQuery(query, [], pConn);
end;

class function TDB.SimpleQuery(const query: String; arr_params: array of Variant; pConn: TDBConn = dbLocal): TFDQuery;
begin

  try
    result := self.SQLExec(TSqlReadData, query, arr_params, pConn);
  except
    on e: exception do
      raise exception.Create(e.Message);
  end;

end;

class function TDB.TryConnect(server, DB: string): boolean;
var
  con: TfdConnection;
begin

  if server.IsEmpty then
    raise exception.Create('Servidor para conexão com banco de dados deve ser informado!');

  if DB.IsEmpty then
    raise exception.Create(format('Banco de dados para conexão com o servidor "%s" deve ser informado!', [server]));

  try
    try
      result := false;
      con := TfdConnection.Create(nil);

      con.Connected := false;
      con.Params.BeginUpdate;
      con.Params.Clear;
      con.Params.endUpdate;

      con.ResourceOptions.AutoReconnect := false;
      con.Params.BeginUpdate;

      con.LoginPrompt := false;
      con.Params.Values['Server'] := server;
      con.Params.Values['Port'] := '3306';
      con.Params.Values['Database'] := DB;
      con.Params.Values['User_name'] := tEnv.database.local.user;
      con.Params.Values['Password'] := tEnv.database.local.password;

      con.Params.Values['DriverID'] := 'MySQL';
      con.Params.Values['LoginTimeout'] := '5000';

      con.Params.endUpdate;
      con.Connected := True;
      result := True;
    except
      on e: exception do
        raise exception.Create('Erro ao conectar com banco de dados:' + e.Message);
    end;
  finally
    con.Free;
  end;

end;

class function TDB.ExecQuery(const query: String; pConn: TDBConn = dbLocal): TFDQuery;
