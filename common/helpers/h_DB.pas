      DELIMITER //
      CREATE TRIGGER  NFE_SEQUENCE  BEFORE INSERT ON  NFE  FOR EACH ROW BEGIN DECLARE SEQ_SERIE INT;
      DECLARE COD_NF INT;
      DECLARE LOOP_AGAIN INT(1);
      SET COD_NF = NEW.NUMERO;
      IF (NEW.ID_EMITENTE = 0) THEN
      SET SEQ_SERIE = (SELECT SEQUENCIA FROM SERIE_SEQUENCIA WHERE SERIE = NEW.SERIE);
      IF (SEQ_SERIE IS NULL) THEN
      INSERT INTO SERIE_SEQUENCIA(SERIE, SEQUENCIA, ULTIMA_NF)VALUES(NEW.SERIE, 1, 1);
      SET SEQ_SERIE = 1;
      END
      IF;
      SET COD_NF = SEQ_SERIE;
      SET LOOP_AGAIN = 1;
      LOOP_FUN:
      REPEAT
      IF ((SELECT NUMERO FROM NFE WHERE NUMERO = COD_NF AND SERIE = NEW.SERIE AND ID_EMITENTE = 0) IS NULL) THEN
      SET COD_NF = COD_NF;
      UPDATE SERIE_SEQUENCIA SET ULTIMA_NF = COD_NF WHERE SERIE = NEW.SERIE;
      SET LOOP_AGAIN = 0;
      ELSE
      SET COD_NF = COD_NF + 1;
      SET LOOP_AGAIN = 1;
      END
      IF;
