class procedure TDB.CreateTable(TableName: string; columns: array of string);
var
  script, c: string;
begin
  script := 'create table ' + TableName + ' (';
  for c in columns do
    script := script + c + ',';
  script := copy(script, 0, length(script) - 1) + ')';
  ExecQuery(script);
end;

